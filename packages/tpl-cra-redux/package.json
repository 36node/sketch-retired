{
  "name": "@36node/template-cra-redux",
  "version": "0.11.0",
  "description": "A react boilerplate based on CRA",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "36node/tpl-react-redux",
    "type": "git"
  },
  "template": "cra-redux",
  "files": [
    ".circleci",
    ".storybook",
    "bin",
    "mock",
    "public",
    "src",
    "stories",
    "antd.theme.js",
    "Dockerfile",
    "README.md"
  ],
  "config-overrides-path": "../cli/config-overrides",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "REACT_APP_VERSION=$npm_package_version sketch build",
    "format": "prettier --trailing-comma es5 --write 'src/**/*.js'",
    "lint": "sketch lint src",
    "release": "standard-version",
    "server": "serve -s build",
    "start": "REACT_APP_VERSION=$npm_package_version sketch start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "sketch test --env=jsdom"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && CI=true NODE_ENV=test npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@36node/browserslist-config": "^0.1.1",
    "@36node/eslint-config": "^0.6.1",
    "@36node/sketch": "^0.11.12",
    "@commitlint/cli": "^6.1.0",
    "@commitlint/config-conventional": "^6.1.0",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-notes": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-responsive": "^3.0.3",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.17.1",
    "standard-version": "^6.0.1"
  },
  "dependencies": {
    "@36node/fetch": "^0.6.0",
    "@36node/redux-api": "^0.2.0",
    "@36node/redux-form-antd": "^0.0.3",
    "@36node/redux-ui": "^0.0.10",
    "@36node/redux-xlsx": "^0.0.4",
    "@36node/template-react-component": "^0.3.15",
    "@36node/template-sdk": "^0.9.17",
    "@types/jest": "^24.0.11",
    "antd": "^3.15.1",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "normalizr": "^3.3.0",
    "react": "^16.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.11.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.3"
  },
  "browserslist": [
    "extends @36node/browserslist-config"
  ]
}
